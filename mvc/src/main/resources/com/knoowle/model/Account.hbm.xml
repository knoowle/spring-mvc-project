<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.knoowle.model">
	<class name="Account">
		<id name="id" column="account_id">
			<generator class="native"/>
		</id>
		
		<property name="name" column="account_name"/>
		<property name="email" column="account_email"/>
		<property name="password" column="account_password"/>
		<property name="registerDate" column="register_date" type="java.util.Date"/>
	</class>
</hibernate-mapping>